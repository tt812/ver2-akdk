        compatible = "zmk,keymap";

        layer_0 {
            bindings = <
&kp Q      &kp W  &kp E  &kp R  &kp T        &kp LS(LC(B))          &kp DELETE       &kp Y      &kp U      &kp I        &kp O          &kp P
